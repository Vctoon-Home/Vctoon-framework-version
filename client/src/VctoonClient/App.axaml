<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:vctoonClient="clr-namespace:VctoonClient"
             xmlns:labs="clr-namespace:Avalonia.Labs.Controls;assembly=Avalonia.Labs.Controls"
             xmlns:navigations="clr-namespace:VctoonClient.Navigations"
             xmlns:router="clr-namespace:VctoonClient.Navigations.Router"
             xmlns:dialogHostAvalonia="clr-namespace:DialogHostAvalonia;assembly=DialogHost.Avalonia"
             xmlns:dialogs="clr-namespace:VctoonClient.Dialogs"
             xmlns:i="https://github.com/projektanker/icons.avalonia"
             xmlns:avalonia="clr-namespace:EasyDialog.Avalonia;assembly=EasyDialog.Avalonia"
             x:Class="VctoonClient.App"
             RequestedThemeVariant="Default">
    <!-- "Default" ThemeVariant follows system theme variant. "Dark" or "Light" are other available options. -->
    <!-- <Application.DataTemplates> -->
    <!--     <vctoonClient:ViewLocator/> -->
    <!-- </Application.DataTemplates> -->
    <Application.Styles>
        <FluentTheme />
        <labs:ControlThemes />
        <StyleInclude Source="avares://Semi.Avalonia/Themes/Index.axaml" />
        <StyleInclude Source="avares://Ursa.Themes.Semi/Index.axaml" />
        <avalonia:EasyDialog />
    </Application.Styles>
</Application>
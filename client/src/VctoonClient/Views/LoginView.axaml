<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:t="clr-namespace:Abp.Localization.Avalonia;assembly=Abp.Localization.Avalonia"
             xmlns:com="clr-namespace:VctoonClient.Components;assembly=VctoonClient"
             xmlns:viewModels="clr-namespace:VctoonClient.ViewModels"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:DataType="viewModels:LoginViewModel"
             x:Class="VctoonClient.Views.LoginView">
    <StackPanel>
        <com:LanguageSelect></com:LanguageSelect>

        <Button Command="{Binding Login}">Login</Button>

        <TextBlock Text="{t:Localized 'Save'}"> </TextBlock>
        <TextBox></TextBox>
    </StackPanel>
</UserControl>